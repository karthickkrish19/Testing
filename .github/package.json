{
  "name": "elvis",
  "version": "5.2.0",
  "private": true,
  "homepage": "http://localhost:8080/elvis-ui",
  "dependencies": {
    "@azure/msal-browser": "^3.28.1",
    "@azure/msal-react": "^2.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^4.5.8",
    "@microsoft/applicationinsights-react-js": "^17.3.6",
    "@microsoft/applicationinsights-web": "^3.3.9",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.176",
    "@mui/material": "^5.17.1",
    "@mui/x-data-grid": "^6.20.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "cronstrue": "^2.61.0",
    "nth-check": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-h5-audio-player": "^3.10.0",
    "react-js-cron": "^4.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=8080 PUBLIC_URL=http://localhost:8080/elvis-ui react-scripts start",
    "build": "react-scripts build",
    "build:prod": "react-scripts build -p",
    "test": "react-scripts test --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "test:watch": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/hooks/",
      "!<rootDir>/src/**/__mock__/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/react-app-env.d.ts",
      "<rootDir>/src/reportWebVitals.ts",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/setupTests.*",
      "<rootDir>/src/compiler/types.*",
      "<rootDir>/src/assets/images/",
      "<rootDir>/src/providers",
      "<rootDir>/src/services",
      "<rootDir>/src/hooks/useInterval.ts",
      "<rootDir>/src/hooks/useLoader.ts",
      "<rootDir>/src/configuration/theme",
      "<rootDir>/src/hooks",
      "<rootDir>/src/components/authentication/",
      "<rootDir>/src/pages/vendor/",
      "<rootDir>/src/pages/clientConfiguration/types.ts",
      "<rootDir>/src/pages/clientDistributions/types.ts",
      "<rootDir>/src/pages/common/types.ts",
      "<rootDir>/src/pages/memberTimeline/types/",
      "<rootDir>/src/pages/memberTimeline/Step.tsx",
      "<rootDir>/src/pages/clientConfiguration/EditClientConfig.tsx",
      "<rootDir>/src/pages/vendor/types.ts",
      "<rootDir>/src/pages/rreidLookup/",
      "<rootDir>/src/pages/vendor/schemas/",
      "<rootDir>/src/types/",
      "<rootDir>/src/utils/index.ts",
      "<rootDir>/src/components/errorPages/",
      "<rootDir>/src/components/common/Loader.tsx",
      "<rootDir>/src/configuration/feature/"
    ],
    "moduleNameMapper": {
      "^react($|/.+)": "<rootDir>/node_modules/react$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
